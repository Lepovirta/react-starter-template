{
  "name": "reactProjectTemplate",
  "version": "0.1.0",
  "description": "Project template that sets up a fully functioning react.js application with webpack.",
  "main": "assets/js/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mvilrokx/react-starter-template.git"
  },
  "keywords": [
    "template",
    "react",
    "webpack",
    "hot module replacement",
    "ES6",
    "quickstart"
  ],
  "devDependencies": {
    "autoprefixer-core": "^5.1.7",
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "css-loader": "^0.9.1",
    "csswring": "^3.0.2",
    "jquery": "^2.1.3",
    "ncp": "^2.0.0",
    "postcss-loader": "^0.3.0",
    "react": "^0.13.1",
    "react-hot-loader": "^1.2.3",
    "sass-loader": "^0.5.0",
    "style-loader": "^0.8.3",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.7.0"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.production.config.js",
    "dev": "webpack-dev-server --progress --colors --hot --content-base dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm ./dist/assets/js/bundle.js ./dist/index.html"
  },
  "author": "Mark Vilrokx <mvilrokx@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mvilrokx/react-starter-template/issues"
  },
  "homepage": "https://github.com/mvilrokx/react-starter-template",
}
